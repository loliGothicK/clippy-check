{
    "name": "clippy-check",
    "version": "0.3.1",
    "description": "\"GitHub Action for PR annotations with clippy checks\"",
    "main": "lib/src/main.js",
    "scripts": {
        "build": "tsc",
        "format": "prettier --check --ignore-path .gitignore src",
        "format:fix": "prettier --write --ignore-path .gitignore src",
        "lint": "eslint src/**/*.ts",
        "lint:fix": "eslint --fix src/**/*.ts",
        "pack": "ncc build --source-map --license LICENSE",
        "test": "jest --runInBand",
        "all": "npm run format:fix && npm run lint:fix && npm run build && npm run pack"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/LoliGothick/clippy-check.git"
    },
    "keywords": [
        "actions",
        "rust",
        "cargo"
    ],
    "author": "Mitama Lab",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/LoliGothick/clippy-check/issues"
    },
    "homepage": "https://github.com/LoliGothick/clippy-check#readme",
    "devDependencies": {
        "@types/core-js": "2.5.5",
        "@types/node": "18.17.6",
        "@typescript-eslint/eslint-plugin": "6.4.0",
        "@typescript-eslint/parser": "6.4.0",
        "@vercel/ncc": "0.36.1",
        "eslint": "8.47.0",
        "eslint-config-prettier": "8.8.0",
        "eslint-config-standard-with-typescript": "37.0.0",
        "eslint-plugin-github": "4.9.2",
        "eslint-plugin-import": "2.28.0",
        "eslint-plugin-jest": "27.2.3",
        "eslint-plugin-n": "16.0.1",
        "eslint-plugin-promise": "6.1.1",
        "prettier": "3.0.0",
        "typescript": "5.1.6"
    },
    "dependencies": {
        "@actions/core": "1.10.0",
        "@actions/exec": "1.1.1",
        "@actions/github": "5.1.1",
        "outdent": "0.8.0",
        "string-argv": "0.3.2"
    },
    "volta": {
        "node": "18.17.1",
        "yarn": "3.6.1"
    },
    "packageManager": "yarn@3.6.1"
}
